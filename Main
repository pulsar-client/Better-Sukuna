-- [[ GOLDENHEAD: PUBLIC SERVER STABILITY FIX v3 ]]

local function applyMapFix()
    local map = workspace:FindFirstChild("Map")
    if not map then return end
    
    -- Added "Stuff" based on your latest error
    local targetFolders = {"GrassTop", "GrassBottom", "Stuff"}
    
    for _, name in pairs(targetFolders) do
        local folder = map:FindFirstChild(name)
        if folder then
            -- Create a "Union" part if missing
            if not folder:FindFirstChild("Union") then
                local u = Instance.new("Part", folder)
                u.Name = "Union"; u.Transparency = 1; u.Anchored = true; u.CanCollide = false
                u.Position = Vector3.new(0, -500, 0)
            end
            -- Create a generic "Part" if missing (for the "Stuff" folder error)
            if not folder:FindFirstChild("Part") then
                local p = Instance.new("Part", folder)
                p.Name = "Part"; p.Transparency = 1; p.Anchored = true; p.CanCollide = false
                p.Position = Vector3.new(0, -500, 0)
            end
        end
    end
    print("Map Fix Applied: Top, Bottom, and Stuff patched.")
end

-- Run the fix
applyMapFix()

-- Load the original script
loadstring(game:HttpGet("https://raw.githubusercontent.com/Reapvitalized/TSB/refs/heads/main/GOLDENHEAD.lua"))()
