-- Services
local HttpService = game:GetService("HttpService")
local StarterGui = game:GetService("StarterGui")

-- Parallel Loading Function
local function quickLoad(url)
    task.spawn(function()
        local success, result = pcall(function()
            return game:HttpGet(url, true)
        end)

        if success and result then
            local func, err = loadstring(result)
            if func then
                task.spawn(func) -- Run the script in its own thread
            else
                warn("Syntax Error in script:", url, "|", err)
            end
        else
            warn("Connection Error:", url)
        end
    end)
end

-- Helper for notifications
local function notify(title, text)
    StarterGui:SetCore("SendNotification", {
        Title = title;
        Text = text;
        Duration = 3;
    })
end

-- Check if game is loaded (Non-blocking)
if not game:IsLoaded() then
    notify("Delta Loader", "Waiting for game to finish loading...")
    game.Loaded:Wait()
end

-- Execute all scripts simultaneously
local scripts = {
    "https://raw.githubusercontent.com/pulsar-client/Better-Sukuna/refs/heads/main/Spawn",
    "https://raw.githubusercontent.com/Mark22028-2ndAcc/Scripts/refs/heads/main/OPCamlock.lua",
    "https://raw.githubusercontent.com/Gughejd/Refesus/refs/heads/main/NYANSKEN.lua.txt"
}

for _, url in ipairs(scripts) do
    quickLoad(url)
end

notify("Success", "All scripts injected!")
